<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.callingsos">
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_sos"
        android:label="@string/app_name"
        android:usesCleartextTraffic="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/ic_sos"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:targetApi="m">
        <activity android:name=".frontend.Incoming_audio_call"></activity>
        <activity android:name=".frontend.AudioCallScreen" />
        <activity android:name=".frontend.AudioCall"
            android:theme="@style/AppTheme1" />
        <activity android:name=".addlocation.Editor" />
        <activity android:name=".login.Registre" />
        <activity android:name=".login.Login" />
        <activity android:name=".addlocation.Addlocation" />
        <activity
            android:name=".menu.Menu"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".frontend.MainActivity"
            android:theme="@style/AppTheme1" /> <!-- Running Application -->
        <activity android:name=".loading.LoadingApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>